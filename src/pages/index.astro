---
import HeadCommon from '@components/HeadCommon';
import HeadSEO from '@components/HeadSEO';

const { content = {} } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
  <HeadCommon />
  <HeadSEO {content} canonicalURL={Astro.canonicalURL} />  
  

  <style>
    /* base */
    :root {
      --gap: 1.5rem;
      --content-width: 1200px;
      --content-width-narrow: 900px;

      --color-txt: #361719;
      --color-txt-contrast: #fff;

      --color-neutral: #fff;
      --color-secondary: #E9EAF7;
      --color-primary: #0d5c63;
      --color-primary-offset: #167073;
    }

    html {
      scroll-behavior: smooth;
    }

    body {
      font-family: Work Sans, -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", sans-serif;
      line-height: 1.5;
      padding: 0;
      margin: 0;
      color: var(--color-txt);
    }

    * {
      box-sizing: border-box;
    }

    h1, h2, h3, h4, h5, p {
      margin-block-start: 0;
    }

    h1, h2, h3, h4, h5 {
      font-family: PT Mono, monospace;
      color: var(--color-primary);
    }

    main li+li {
      margin-block-start: .4rem;
    }

    a {
      color: var(--color-primary);
      text-decoration: underline;
      text-underline-offset: .2em;
    }

    img {
      width: 100%;
      height: auto;
    }

    .container {
      padding-block: clamp(3rem, 7vw, 8rem);
      padding-inline: 2rem;
      max-width: var(--content-width);
      margin: 0 auto;
    }

    .container.is-narrow {
      max-width: var(--content-width-narrow);
    }

    .container.is-hero {
      padding-block-end: 8rem;
    }




    /* Utils */
    .has-padding-t-0 {
      padding-top: 0 !important;
    }

    .has-txt-center {
      text-align: center;
    }

    .has-txt-bold {
      font-weight: 700;
    }

    .txt-primrary {
      color: var(--color-primary);
    }

    .has-bg-neutral {
      background-color: var(--color-neutral);
    }

    .has-bg-secondary {
      background-color: var(--color-secondary);
    }

    .has-bg-primary {
      background-color: var(--color-primary);
    }

    .has-bg-primary * {
      color: var(--color-txt-contrast);
    }




    /* Cols */
    @media screen and (min-width:700px) {
      .cols {
        display: flex;
        flex-direction: row;
        justify-content: space-between;
      }

      .col {
        flex: 0 0 50%;
      }
    }





    /* button */
    .btn {
      text-decoration: none;
      background-color: var(--color-primary);
      color: var(--color-txt-contrast);
      padding: .5rem 1.17rem;
      border-radius: 300px;
      font-size: 1.1rem;
      transition: .2s background-color linear;
      display: inline-block;
    }
    .btn:hover {
      background-color: var(--color-primary-offset);
    }





    /* SVG */
    .logo, .icon {
      position: relative;
      display: inline-block;
      box-sizing: content-box;
      line-height: 1;
      fill: currentColor;
    }

    .icon {
      width: 25px;
      height: 25px;
    }

    .logo {
      width: 170px;
      height: 50px;
    }




    /* Hero */
    .hero-tagline {
      font-weight: 700;
      line-height: 1.5;
      font-size: clamp(2rem, 3.6vw, 3.2rem);
    }



    /* Navbar */
    .navbar {
      padding-block: 2rem;
      padding-inline: 2rem;
      max-width: var(--content-width);
      margin: 0 auto;

      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      align-items: center;
      column-gap: 1rem;
    }

    .navbar-logo {
      max-width: 170px;
      margin: 0;
      line-height: 1;
    }

    .navbar-links {
      display: flex;
      gap: 25px;
      justify-content: center;
      align-items: center;
      padding-block: .4rem;
      padding-inline: 1rem;
      list-style-type: none;
    }

    .navbar-links a {
      text-decoration: none;
      font-size: .9rem;
      vertical-align: middle;
    }

    @media screen and (max-width:600px) {
      .navbar-links.is-contained-mobile {
        border-radius: 300px;
        background-color: var(--color-primary-offset);
      }

      .navbar-links.is-contained-mobile a {
        color: var(--color-txt-contrast);
      }
    }

    @media screen and (min-width:600px) {
      .navbar {
        justify-content: space-between;
      } 

      .navbar-links.is-contained-mobile {
        background-color: transparent;
      }

      .navbar .icon {
        fill: var(--color-primary);
      }

      .navbar-links a {
        font-size: 1.1rem;
      }
    }



    /* sponsor logos */
    .logos {
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      align-items: center;
    }

    .logos>* {
      padding-inline: 2rem;
    }

    .logos img {
      max-width: 200px;
    }
  </style>
</head>
<body>
  <header class="has-bg-secondary">
    <div class="navbar">
      <h1 class="navbar-logo">
        <a href="/" class="logo">
          <svg viewBox="0 0 170 50">
            <title>Email Markup Consortium</title>
            <use xlink:href="#logo-emc" width="170" height="48"></use>
          </svg>
        </a>
      </h1>

      <ul class="navbar-links is-contained-mobile">
        <li>
          <a href="/en/docs/">Docs</a>
        </li>
        <li>
          <a href="https://github.com/email-markup-consortium/email-markup-consortium/discussions">Forum</a>
        </li>
        <li>
          <a href="https://dev.to/emailmarkup">Blog</a>
        </li>

        <li>
          <a class="icon" href="https://twitter.com/emailmarkup" target="_blank" aria-label="Twitter">
            <svg viewBox="23 23 64 64">
              <use xlink:href="#icon-twitter" width="110" height="110"></use>
            </svg>
          </a>
        </li>
        
        <li>
          <a class="icon" href="https://github.com/email-markup-consortium/" target="_blank" aria-label="GitHub">
            <svg viewBox="23 23 64 64">
              <use xlink:href="#icon-github" width="110" height="110"></use>
            </svg>
          </a>
        </li>
      </ul>
    </div>

    <div class="container is-hero">
      <p class="hero-tagline txt-primrary">
        Working to improve the user experience, accessibility, performance, consistency, and reliability of email markup
      </p>
  
      <a href="#membership" class="btn">Become a Member</a>
    </div>
  </header>

  <main>
    <section class="container">
      <h2>Our Misson</h2>

      <p>
        The Email Markup Consortium (EMC) is a community-led group of industry professionals working to improve the email experience for everyone.
      </p>

      <p>
        Senders, recipients, and email clients share the common goals of improving:
      </p>

      <ul>
        <li>user experience</li>
        <li>accessibility</li>
        <li>performance</li>
        <li>consistency</li>
        <li>reliability</li>
      </ul>

      <p>
        However, currently there is a lack of cooperation in trying to achieve these goals leading to inconsistent results. By working together on agreed approaches we can save everyone time, effort, and money while delivering better results to the recipients.
      </p>

      <p>
        Recipients should be able to easily access the content of every email sent to them, regardless of their device, email client, assistive technology or internet speed. The EMC collaborates with developers, creation and sending tools, and email clients to drive this vision forward at every step of the email journey.
      </p>

      <a href="/en/docs/" class="btn">Read the docs</a>
    </section>


    <section id="membership" class="has-bg-secondary">
      <div class="container">
        <h2>How You Can Help</h2>
  
        <div class="cols">
          <div class="col">
            <h3>Join Us</h3>

            <ul>
              <li>
                <a class="has-txt-bold" href="https://forms.gle/XVdT3JGUbLZQEHXu6">
                  Individual membership application form
                </a>
              </li>
              <li>
                <a class="has-txt-bold" href="https://forms.gle/yifokDd3rfdFRG7i8">
                  Corporate membership application form
                </a>
              </li>
              <li>
                Not ready to apply? <a class="has-txt-bold" href="https://github.com/email-markup-consortium/email-markup-consortium/discussions">Join the discussion on GitHub</a>
              </li>
            </ul>
          </div>


          <div class="col">
            <h3>Spread the Word</h3>

            <ul>
              <li>
                <a href="https://twitter.com/intent/tweet?url=https%3A%2F%2Femailmarkup.org">
                  Twitter
                </a>
              </li>
              <li>
                <a href="https://www.linkedin.com/shareArticle?mini=true&url=https%3A%2F%2Femailmarkup.org">
                  LinkedIn
                </a>
              </li>
              <li>
                <a href="https://www.facebook.com/sharer?u=https%3A%2F%2Femailmarkup.org">
                  Facebook
                </a>
              </li>
            </ul>
          </div>
        </div>

      </div>
    </section>

    
    <section class="has-bg-primary has-txt-center">
      <div class="container is-narrow">
        <h2>Supporters</h2>
        <p>
          The Email Markup Consortium and its mission are supported by employees of Salesforce, Squarespace, Litmus, Email on Acid, SparkPost, Constant Contact, ActionRocket, Parcel, GitHub, and more.
        </p>
      </div>

      <div class="container has-padding-t-0">
        <h2 class="has-txt-center">Sponsors</h2>
        <div class="logos">
          <a href="https://parcel.io">
            <img src="img/logo-parcel.png" alt="Parcel">
          </a>

          <a href="https://www.mailjet.com">
            <img src="img/logo-mailjet.png" alt="Mailjet">
          </a>
        </div>
      </div>
    </section>
  </main>

  <footer class="navbar">
      <div class="navbar-logo">
        <a href="/" class="logo">
          <svg viewBox="0 0 170 50">
            <title>Email Markup Consortium</title>
            <use xlink:href="#logo-emc" width="170" height="48"></use>
          </svg>
        </a>
      </div>

      <ul class="navbar-links">
        <li>
          <a href="https://dev.to/emailmarkup">Blog</a>
        </li>
        <li>
          <a href="https://dev.to/feed/emailmarkup">RSS</a>
        </li>
        <li>
          <a href="https://forms.gle/fQ7eC6YwABDGTrYQ9">Contact</a>
        </li>
        
        <li>
          <a class="icon" href="https://twitter.com/emailmarkup" target="_blank" aria-label="Twitter">
            <svg viewBox="23 23 64 64">
              <use xlink:href="#icon-twitter" width="110" height="110"></use>
            </svg>
          </a>
        </li>
        
        <li>        
          <a class="icon" href="https://github.com/email-markup-consortium/" target="_blank" aria-label="GitHub">
            <svg viewBox="23 23 64 64">
              <use xlink:href="#icon-github" width="110" height="110"></use>
            </svg>
          </a>
        </li>
      </ul>
  </footer>

